    <script>
      function openNewTab(url) {
      const newTab = window.open('about:blank');
      let fav = localStorage.getItem('fav')
      let title = localStorage.getItem('title')
      newTab.document.write(`<title>${title ? title : 'about:blank'}</title><link rel="icon" href="${fav ? fav : 'data:;'}"><body style="margin:0px;"><iframe src="${url || window.location.href}" frameborder="0" width="100%" height="100%" style="width:100vw;height:100vh;"></iframe></body>`);
      newTab.document.close();
    }
    function changeTab(name, favicon) {
      document.title = name || 'Game0';
      document.querySelector('link[rel=icon]').href = favicon || 'https://i.ibb.co/y6L3X97/image.png';
      localStorage.setItem('fav', favicon);
      localStorage.setItem('title', name)
    }

      function set(){
        changeTab(document.getElementById('title').value, document.getElementById('fav').value)
      }
    </script>
